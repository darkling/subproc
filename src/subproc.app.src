{application, subproc, [
  {description, "unix subprocess manager"},
  {vsn, "0.1.0"},
  {registered, [
    subproc_sup,
    subproc_driver_loader,
    subproc_mdrv_reaper,
    subproc_master
  ]},
  {applications, [kernel, stdlib]},
  {modules, []},
  {mod, {subproc_app, []}},
  {env, [
    {shutdown_timeout, infinity},
    {shutdown_kill, false}
  ]}
]}.
